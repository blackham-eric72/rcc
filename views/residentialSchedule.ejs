                        <div class="day"></div>
                        <div class="day"></div>
                        <div class="day"></div>
                        <div class="day"></div>
                </div>
            </div>
        </div>

        <div class="dailySchedule" id="dailySchedule">
            <h3>Available times for <span id="scheduleDate"></span></h3>
            <p>*available times appear green</p>
            <div class="timeSlot available"><p class="hour">9:00am</p></div>
            <div class="timeSlot booked "><p class="hour">10:00am</p></div>
            <div class="timeSlot booked"><p class="hour">11:00am</p></div>
            <div class="timeSlot booked"><p class="hour">12:00pm</p></div>
            <div class="timeSlot booked"><p class="hour">1:00pm</p></div>
            <div class="timeSlot booked"><p class="hour">2:00pm</p></div>
            <div class="timeSlot booked"><p class="hour">3:00pm</p></div>
            <div class="timeSlot booked"><p class="hour">4:00pm</p></div>
        </div>
    </div>


    <section class="section section-light" id="one">
        <h3>Please fill out the form to get a custom estimate for your home!</h3>
            <div class="container">
                    <form id ="estimateForm" name="estimateForm" enctype="multipart/form-data" action="form_submit">
                  
                      <label for="fname">First Name</label>
                      <input type="text" id="fname" name="firstname" placeholder="Your name..">
                  
                      <label for="lname">Last Name</label>
                      <input type="text" id="lname" name="lastname" placeholder="Your last name..">

                      <label for="lname">Email</label>
                      <input type="email" id="email" name="email" placeholder="Your email..">

                <div class="roomSelect">
                    <div class="selection">
                        <label for="livingRoom">Number of Living Rooms</label>
                        <select id="livingRoom" name="livingRoom">
                            <option value="0">0</option>
                            <option value="1">1</option>
                            <option value="2 ">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                            <option value="5">6</option>
                            <option value="5">7</option>
                        </select>
                    </div>
                    <div class="selection">
                      <label for="bedRoom">Number of Bed Rooms</label>
                      <select id="bedRoom" name="bedRoom">
                        <option value="0">0</option>
                        <option value="1">1</option>
                        <option value="2 ">2</option>
                        <option value="3">3</option>
                        <option value="4">4</option>
                        <option value="5">5</option>
                        <option value="5">6</option>
                        <option value="5">7</option>
                      </select>
                    </div>

                      <div class="selection">
                        <label for="hallway">Number of Hallways</label>
                        <select id="hallway" name="hallway">
                            <option value="0">0</option>
                            <option value="1">1</option>
                            <option value="2 ">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                            <option value="5">6</option>
                            <option value="5">7</option>
                        </select>
                      </div>

                      <div class="selection">
                        <label for="stairCase">Number of Stair Cases</label>
                        <select id="stairCase" name="stairCase">
                            <option value="0">0</option>
                            <option value="1">1</option>
                            <option value="2 ">2</option>
                            <option value="3">3</option>
                            <option value="4">4</option>
                            <option value="5">5</option>
                            <option value="5">6</option>
                            <option value="5">7</option>
                        </select>
                      </div>
                    </div>
                  
                      <label for="subject">Comments / Questions </label>
                      <textarea id="subject" name="subject" placeholder="Write something.." style="height:100px"></textarea>
                      <input type="submit" value="Submit">
                  
                    </form>
                  </div> 
    </section>
